.panel{
  padding:15px 10px;
}
.log {
	color: #999;
  background-color: transparent;
}
.log_item{
	padding: 15px 10px;
  margin-bottom:10px;
  background-color:#fff;
}
.log_item text {
	padding-right: 20px;
}
.clear{
	position: fixed;
	bottom: 0px;
	width: 100%;
  text-align: right;
}
.clear .clear_btn {
	margin: 10px;
  width: 50px;
	height: 50px;
	line-height: 50px;
	color:#fff;
	background-color: #fd8f58;
	border-radius: 50%;
}
